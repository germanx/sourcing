<%= form_for [@firm, @employee], :html => { :multipart => true } do |f| %>
  <%= f.error_messages %>
  
  <fieldset>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :position %>
    <%= f.text_field :position %>
   
    <%= f.label :email %>
    <%= f.text_field :email %>
   
    <%= f.label :phone %>
    <%= f.text_field :phone %>
  </fieldset>
  
  <%= f.submit %>
<% end %>